    <sys:String x:Key="LOCSteam_Launcher_UtilityDisableSteamWebBrowserOnDesktopModeSetting">Deaktiver Steam-nettleser når du starter et spill i skrivebordsmodus</sys:String>
    <sys:String x:Key="LOCSteam_Launcher_UtilityLaunchSteamBpmOnDesktopModeSetting">Start Steam i Big Picture Mode når du starter et spill i skrivebordsmodus</sys:String>
    <sys:String x:Key="LOCSteam_Launcher_UtilityDisableSteamWebBrowserOnFullscreenModeSetting">Deaktiver Steam-nettleser når du starter et spill i fullskjermsmodus</sys:String>
    <sys:String x:Key="LOCSteam_Launcher_UtilityLaunchSteamBpmOnFullscreenModeSetting">Start Steam i Big Picture Mode når du starter et spill i fullskjermsmodus</sys:String>
    <sys:String x:Key="LOCSteam_Launcher_UtilityCloseSteamIfRunningSetting">Lukk Steam hvis programmet kjører før spillet startes</sys:String>
    
