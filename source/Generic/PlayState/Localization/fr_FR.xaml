    <sys:String x:Key="LOCPlayState_HourMinutesPlayed">{0} heure {1} minutes</sys:String>
    <sys:String x:Key="LOCPlayState_HourMinutePlayed">{0} heure {1} minute</sys:String>
    <sys:String x:Key="LOCPlayState_SecondsPlayed">{0} secondes</sys:String>
    <sys:String x:Key="LOCPlayState_SecondPlayed">{0} seconde</sys:String>
