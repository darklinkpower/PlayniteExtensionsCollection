    <sys:String x:Key="LOCPlayState_HourMinutesPlayed">{0} година {1} хвилин</sys:String>
    <sys:String x:Key="LOCPlayState_HourMinutePlayed">{0} година {1} хвилина</sys:String>
    <sys:String x:Key="LOCPlayState_SecondsPlayed">{0} секунд</sys:String>
