    <sys:String x:Key="LOCGame_Sizes_Calculator_DialogMessageDone">完成</sys:String>

    <sys:String x:Key="LOCGame_Sizes_Calculator_NotificationMessageErrorGetDirSize">Games Size Calculator：在 {0} 中获取游戏 {1} 的目录大小时出错。{2}</sys:String>
    <sys:String x:Key="LOCGame_Sizes_Calculator_NotificationMessageErrorGetRomFileSize">Games Size Calculator：在 {0} 中获取游戏 {1} 的 ROM 文件大小时出错。{2}</sys:String>
